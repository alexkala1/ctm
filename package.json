    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.3",
    "jspdf-autotable": "^5.0.2",
    "lodash-es": "^4.17.21",
    "next-auth": "^4.24.11",
    "nodemailer": "^6.9.7",
    "nuxt": "^4.1.2",
    "nuxt-viewport": "^2.3.1",
    "papaparse": "^5.4.1",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^4.5.0",
    "typescript": "^5.9.2",
