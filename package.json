{
  "name": "nuxt-app",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --fix --cache",
    "format": "eslint . --fix --cache",
    "format:check": "eslint . --cache",
    "format:lint": "npm run format",
    "lint:fast": "eslint . --cache --max-warnings 0 --quiet",
    "format:fast": "eslint . --fix --cache --max-warnings 0 --quiet",
    "db:generate": "dotenv -e .env -- prisma generate",
    "db:push": "dotenv -e .env -- prisma db push",
    "db:migrate": "dotenv -e .env -- prisma migrate dev",
    "db:seed": "dotenv -e .env -- tsx scripts/seed.ts",
    "db:reset": "dotenv -e .env -- prisma migrate reset --force && npm run db:seed",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:all": "npm run test && npm run test:e2e"
  },
  "dependencies": {
    "@faker-js/faker": "^10.0.0",
